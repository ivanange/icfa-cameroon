<h1 id="team" class="py-3 text-4xl font-semibold text-gray-900 text-center mt-8 uppercase anchor-fix">L'Ã©quipe</h1>
<div id="slider" class="slides w-full py-10 flex relative justify-evenly max-w-screen-2xl mb-10">
	<div id="slides" class="flex-grow-1 overflow-x-hidden flex justify-evenly min-w-full">
		{% for member in team %}
			<div class="bg-cover bg-center flex flex-col justify-center items-center w-screen sm:w-auto flex-shrink-0 max-w-xs text-center" style="min-width:250px;">
				<div class="w-48 h-48 rounded-full bg-gray-200 border-2 border-gray-300 bg-cover bg-center" style="background-image: url(/assets/img/profiles/{{member.image|default('anonymous.jpg', true)}});"></div>
				<h3 class="text-gray-900 font-bold text-2xl leading-snug mt-6 pb-1">
					{{member.name}}
				</h3>
				<span class="text-gray-600 leading-tight">{{member.title}}</span>
				<div class="social flex pt-5">
					{% if member.social.email %}
						<a class="block w-8 h-8 p-2 bg-footer-dark focus:bg-social-active focus:text-orange-500 hover:bg-social-active  hover:text-orange-500  rounded-full text-white mr-4 overflow-visible" href="mailto:{{member.social.email}}" title="{{member.social.email}}">
							<svg aria-hidden="true" focusable="false" data-prefix="far" data-icon="envelope" role="img" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 512 512" class="svg-inline--fa fa-envelope fa-w-16 fa-3x">
								<path fill="currentColor" d="M464 64H48C21.49 64 0 85.49 0 112v288c0 26.51 21.49 48 48 48h416c26.51 0 48-21.49 48-48V112c0-26.51-21.49-48-48-48zm0 48v40.805c-22.422 18.259-58.168 46.651-134.587 106.49-16.841 13.247-50.201 45.072-73.413 44.701-23.208.375-56.579-31.459-73.413-44.701C106.18 199.465 70.425 171.067 48 152.805V112h416zM48 400V214.398c22.914 18.251 55.409 43.862 104.938 82.646 21.857 17.205 60.134 55.186 103.062 54.955 42.717.231 80.509-37.199 103.053-54.947 49.528-38.783 82.032-64.401 104.947-82.653V400H48z" class=""></path>
							</svg>
						</a>
					{% endif %}

					{% if member.social.phone %}
						<a class="copy block w-8 h-8 p-2 bg-footer-dark focus:bg-social-active focus:text-orange-500 hover:bg-social-active  hover:text-orange-500  rounded-full text-white mr-4 overflow-visible" href="{{member.social.phone}}" title="{{member.social.phone}}">
							<svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="phone" role="img" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 512 512" class="svg-inline--fa fa-phone fa-w-16 fa-3x">
								<path fill="currentColor" d="M493.4 24.6l-104-24c-11.3-2.6-22.9 3.3-27.5 13.9l-48 112c-4.2 9.8-1.4 21.3 6.9 28l60.6 49.6c-36 76.7-98.9 140.5-177.2 177.2l-49.6-60.6c-6.8-8.3-18.2-11.1-28-6.9l-112 48C3.9 366.5-2 378.1.6 389.4l24 104C27.1 504.2 36.7 512 48 512c256.1 0 464-207.5 464-464 0-11.2-7.7-20.9-18.6-23.4z" class=""></path>
							</svg>
						</a>
					{% endif %}

					{% if member.social.facebook %}
						<a class="block w-8 h-8 p-2 bg-footer-dark focus:bg-social-active focus:text-orange-500 hover:bg-social-active  hover:text-orange-500  rounded-full text-white mr-4" href="{{member.social.facebook}}">
							<svg aria-hidden="true" focusable="false" data-prefix="fab" data-icon="facebook-f" class="fill-current w-full h-full" role="img" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 320 512">
								<path fill="currentColor" d="M279.14 288l14.22-92.66h-88.91v-60.13c0-25.35 12.42-50.06 52.24-50.06h40.42V6.26S260.43 0 225.36 0c-73.22 0-121.08 44.38-121.08 124.72v70.62H22.89V288h81.39v224h100.17V288z"></path>
							</svg>
						</a>
					{% endif %}

					{% if member.social.twitter %}
						<a class="block w-8 h-8 p-2 bg-footer-dark focus:bg-social-active focus:text-orange-500 hover:bg-social-active  hover:text-orange-500  rounded-full text-white mr-4" href="{{member.social.twitter}}">
							<svg aria-hidden="true" focusable="false" data-prefix="fab" data-icon="twitter" class="fill-current w-full h-full" role="img" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 512 512">
								<path fill="currentColor" d="M459.37 151.716c.325 4.548.325 9.097.325 13.645 0 138.72-105.583 298.558-298.558 298.558-59.452 0-114.68-17.219-161.137-47.106 8.447.974 16.568 1.299 25.34 1.299 49.055 0 94.213-16.568 130.274-44.832-46.132-.975-84.792-31.188-98.112-72.772 6.498.974 12.995 1.624 19.818 1.624 9.421 0 18.843-1.3 27.614-3.573-48.081-9.747-84.143-51.98-84.143-102.985v-1.299c13.969 7.797 30.214 12.67 47.431 13.319-28.264-18.843-46.781-51.005-46.781-87.391 0-19.492 5.197-37.36 14.294-52.954 51.655 63.675 129.3 105.258 216.365 109.807-1.624-7.797-2.599-15.918-2.599-24.04 0-57.828 46.782-104.934 104.934-104.934 30.213 0 57.502 12.67 76.67 33.137 23.715-4.548 46.456-13.32 66.599-25.34-7.798 24.366-24.366 44.833-46.132 57.827 21.117-2.273 41.584-8.122 60.426-16.243-14.292 20.791-32.161 39.308-52.628 54.253z"></path>
							</svg>
						</a>
					{% endif %}

					{% if member.social.linkedin %}
						<a class="block w-8 h-8 p-2 bg-footer-dark focus:bg-social-active focus:text-orange-500 hover:bg-social-active  hover:text-orange-500  rounded-full text-white mr-4 overflow-visible" href="{{member.social.linkedin}}">
							<span class="text-center w-full h-full flex flex-center font-bold text-sm leading-none">in</span>
						</a>
					{% endif %}


				</div>
			</div>
		{% endfor %}
	</div>
	<button class="inactive scroll-button absolute text-gray-600 left-0 p-2 outline-none focus:outline-none duration-300 transform hover:scale-105">
		<svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="chevron-left" class="w-8 h-8" role="img" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 320 512">
			<path fill="currentColor" d="M34.52 239.03L228.87 44.69c9.37-9.37 24.57-9.37 33.94 0l22.67 22.67c9.36 9.36 9.37 24.52.04 33.9L131.49 256l154.02 154.75c9.34 9.38 9.32 24.54-.04 33.9l-22.67 22.67c-9.37 9.37-24.57 9.37-33.94 0L34.52 272.97c-9.37-9.37-9.37-24.57 0-33.94z"></path>
		</svg>
	</button>
	<button class="scroll-button absolute text-gray-600 right-0 p-2 pr-0 mr-1 outline-none focus:outline-none duration-300 transform hover:scale-105">
		<svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="chevron-right" class="w-8 h-8" role="img" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 320 512">
			<path fill="currentColor" d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"></path>
		</svg>
	</button>
</div>
